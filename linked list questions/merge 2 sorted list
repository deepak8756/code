#include<bits/stdc++.h>
using namespace std;

class Node{
    public:
    int data;
    Node* next;

    Node(int data){
        this->data=data;
        this->next=NULL;
    }
};

void insertAtTail(Node* &tail,int data)
{
    Node* temp=new Node(data);
    tail->next=temp;
    tail=temp;
}
void print(Node* head)
{
    Node* temp=head;

    while(temp!=NULL)
    {
        cout<<temp->data<<" ";
        temp=temp->next;
    }
    cout<<endl;
}

void mergedata(Node* first,Node*second)
{
    if(first->next==NULL)
    {
        first->next=second;
        return;
    }
    Node* current1=first;
    Node* next1=current1->next;
    Node* current2=second;
    Node* next2=current2->next;

    while(next1!=NULL && current2!=NULL)
    {
        if(current2->data>=current1->data && current2->data<=next1->data)
        {
            //addnodes in between the first nodes
            current1->next=current2;
            next2=current2->next;
            current2->next=next1;
            //updating  pointers below 
            current1=current2;
            current2=next2;
        }
        else{//agar condition false hui to update kro agee bdho
            current1=next1;
            next1=next1->next;
            if(next1==NULL){
                current1->next=current2;
            }
        
        }
      
    }
}

void twoNodes(Node* &first,Node* &second)
{
    if(first->data <= second->data)
    {
        mergedata(first,second);
    }
    else{
        mergedata(second,first);
    }
}

int main()
{
    Node* first=new Node(1);
    Node* head1=first;
    Node* tail1=first;

    insertAtTail(tail1,3);
    insertAtTail(tail1,5);
    print(head1);
    //-------------------------
    Node* second=new Node(2);
    Node* head2=second;
    Node* tail2=second;

    insertAtTail(tail2,4);
    insertAtTail(tail2,5);
    print(head2);

    twoNodes(first,second);
    print(head1);

    

    return 0;
}
