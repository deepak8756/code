#include<bits/stdc++.h>
using namespace std;

class Node{
    public:
    int data;
    Node* next;

    Node(int data){
        this->data=data;
        this->next=NULL;
    }
};

void insertAtTail(Node* &tail,int data){
    Node* temp=new Node(data);

    tail->next=temp;
    tail=temp;
}

void print(Node* head){
    Node* temp=head;

    while(temp!=NULL){
        cout<<temp->data<<" ";
        temp=temp->next;
    }
    cout<<endl;
}

void deleteRepeat(Node* &head){
    Node* current=head;

    while(current!=NULL){
        if((current->next!=NULL) && current->data==current->next->data){
            Node* next_next=current->next->next;//agee ke agee wala
            Node* nodeToDelete=current->next;
            delete(nodeToDelete);
            current->next=next_next;
        }
    else{//agar brabar nhi hai agee wala piche wale ke to simply age bdho aur agee ka check kro
            current=current->next;
        }
    }
   
   
}
int main()
{
    Node* node1=new Node(10);
    Node* head=node1;
    Node* tail=node1;
    insertAtTail(tail,20);
    print(head);
    insertAtTail(tail,20);
    print(head);
    insertAtTail(tail,30);
    print(head);
    insertAtTail(tail,30);
    print(head);
    insertAtTail(tail,50);
    print(head);
     insertAtTail(tail,50);
    print(head);
     insertAtTail(tail,50);
    print(head);
     insertAtTail(tail,60);
    print(head);
    deleteRepeat(head);
    print(head);

    return 0;
}
